[report]
exclude_lines =
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __unicode__
    def __repr__
    def __str__
    if self\.debug

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:

omit =
    */python?.?/*
    */site-packages/*
    *__init__*
    library/settings.py
    */migrations/*

show_missing = True